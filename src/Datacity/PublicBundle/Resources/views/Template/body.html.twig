{% trans_default_domain "body" %}
<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
	<meta charset="utf-8">
	<title>{% block title %}Datacity{% endblock %}</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="description" content="{% block description %}{% endblock %}" />
	<meta name="author" content="{% block author %}{% endblock %}" />
	<!-- css -->
	<link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Noto+Serif:400,400italic,700|Open+Sans:300,400,600,700">
	{% block stylesheets %}
		{% stylesheets
			'css/bootstrap.css'
			'bundles/datacitypublic/js/theme/google-code-prettify/prettify.css'
			'bundles/datacitypublic/css/font-awesome.css'
			'bundles/datacitypublic/css/custom-fonts.css'
			'bundles/datacitypublic/css/overwrite.css'
			'bundles/datacitypublic/css/animate.css'
			'bundles/datacitypublic/css/style.css'
			'bundles/datacitypublic/css/fancybox/jquery.fancybox.css'
			'bundles/datacitypublic/css/jcarousel.css'
			'bundles/datacitypublic/css/flexslider.css'
			'bundles/datacitypublic/css/cslider.css'
			'bundles/datacitypublic/css/skins/blue.css'
			'bundles/datacitypublic/css/datacity.css'
			filter='cssrewrite'
		%}
		    <link rel="stylesheet" href="{{ asset_url }}" />
		{% endstylesheets %}
	{% endblock %}
	<!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
	<!--[if lt IE 9]>
	      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	 <![endif]-->
	<!-- Fav and touch icons -->
	<link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('bundles/datacitypublic/ico/apple-touch-icon-144-precomposed.png') }}" />
	<link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('bundles/datacitypublic/ico/apple-touch-icon-114-precomposed.png') }}" />
	<link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('bundles/datacitypublic/ico/apple-touch-icon-72-precomposed.png') }}" />
	<link rel="apple-touch-icon-precomposed" href="{{ asset('bundles/datacitypublic/ico/apple-touch-icon-57-precomposed.png') }}" />
	<link rel="shortcut icon" href="{{ asset('bundles/datacitypublic/ico/favicon.png') }}" />
</head>
<body {% block bodyattr %}{% endblock %}>
<div id="wrapper" class="boxed">
	<!-- start header -->
	<header>
	<div class="container ">
		<!-- hidden top area toggle link -->
		<div id="header-hidden-link">
			<a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({"_locale": "fr"})) }}"><img src="{{ asset('bundles/datacitypublic/img/flag_fr.jpg') }}" alt="fr" /></a>
			<a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({"_locale": "en"})) }}"><img src="{{ asset('bundles/datacitypublic/img/flag_uk.jpg') }}" alt="en" /></a>
		</div>
		<!-- end toggle link -->
		<div class="row nomargin">
			<div class="span12">
				<!-- On inclut un lien vers le compte utilisateur si la personne est connectÃ© -->
				{% if app.user and is_granted("IS_AUTHENTICATED_REMEMBERED") %}
					{% include 'DatacityPrivateBundle:Include:accountLink.html.twig' %}
				{% else %}
					<div class="headnav">
						<ul>
							<li><a href="{{ path('fos_user_registration_register') }}"><i class="icon-user"></i>{{ 'layout.register'|trans({}, 'FOSUserBundle') }}</a></li>
							<li><a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a></li>
						</ul>
					</div>
				{% endif %}
			</div>
		</div>
		<div class="row">
			<div class="span4">
				<div class="logo">
					<a href="{{ path('datacity_public_homepage') }}"><img src="{{ asset('bundles/datacitypublic/img/logo2.png') }}" alt="" width="110px" class="logo" /></a>
				</div>
			</div>
			<div class="span8">
				<div class="navbar navbar-static-top">
					<div class="navigation">
						<nav>
						<ul class="nav topnav">
							<li class="{% if app.request.attributes.get('_route') == 'datacity_public_homepage' %}active{% endif %}"><a href="{{ path('datacity_public_homepage') }}">{{ 'menu.home'|trans }}</a></li>
							<li class="{% if app.request.attributes.get('_route') == 'datacity_public_dataviewpage' %}active{% endif %}"><a href="{{ path('datacity_public_dataviewpage') }}"><strong>{{ 'menu.datatable'|trans }}</strong></a></li>
							<li class="{% if app.request.attributes.get('_route') starts with 'datacity_public_app' %}active{% endif %}"><a href="{{ path('datacity_public_appportalpage') }}">{{ 'menu.app'|trans }}</a></li>
							<li class="{% if app.request.attributes.get('_route') == 'datacity_public_news' %}active{% endif %}"><a href="{{ path('datacity_public_news') }}">{{ 'menu.news'|trans }}</a></li>
							<li class="{% if app.request.attributes.get('_route') starts with 'datacity_public_producer' %}active{% endif %}"><a href="{{ path('datacity_public_producerpage') }}">{{ 'menu.producers'|trans }}</a></li>
							<li class="{% if app.request.attributes.get('_route') == 'datacity_public_contactpage' %}active{% endif %}"><a href="{{ path('datacity_public_contactpage') }}">{{ 'menu.contact'|trans }}</a></li>
						</ul>
						</nav>
					</div>
					<!-- end navigation -->
				</div>
			</div>
		</div>
	</div>
  </header>
  {% block content %}{% endblock %}
  <footer>
	<div class="container">
		<div class="row">
			<div class="span5">
				<div class="widget">
					<h5 class="widgetheading">{{ 'footer.browse'|trans }}</h5>
					<ul class="link-list">
						<li><a href="{{ path('datacity_public_appportalpage') }}">{{ 'menu.app'|trans }}</a></li>
						<li><a href="{{ path('datacity_public_news') }}">{{ 'menu.news'|trans }}</a></li>
						<li><a href="{{ path('datacity_public_dataviewpage') }}">{{ 'menu.datatable'|trans }}</a></li>
					</ul>
				</div>
			</div>
			<div class="span5">
			    <div class="widget">
				     <h5 class="widgetheading">{{ 'footer.info'|trans }}</h5>
				     <ul class="link-list">
					      <li><a href="{{ path('datacity_public_talkaboutus') }}">{{ 'footer.talkaboutus'|trans }}</a></li>
					      <li><a href="{{ path('datacity_public_condition') }}">{{ 'footer.condition'|trans }}</a></li>
					      <li><a href="{{ path('datacity_public_privacypolicy') }}">{{ 'footer.privacypolicy'|trans }}</a></li>
					    </ul>
			    </div>
			</div>
			<div class="span2">
				<div class="widget">
					<h5 class="widgetheading">{{ 'footer.contact.title'|trans }}</h5>
					<p><a href="{{ path('datacity_public_contactpage') }}">{{ 'footer.contact.form'|trans }}</a></p>
					<p>{{ 'footer.contact.or'|trans }}</p>
					<p><i class="icon-envelope-alt"></i> contact@datacity.com </p>
				</div>
			</div>
		</div>
	</div>
	<div id="sub-footer">
		<div class="container">
			<div class="row">
				<div class="span6">
					<div class="copyright">
						<p>
							<span>{{ 'datacity.copyright'|trans }}
						</p>
					</div>
				</div>
				<div class="span6">
					<ul class="social-network">
						<li><a href="#" data-placement="bottom" title="Facebook"><i class="icon-facebook icon-square"></i></a></li>
						<li><a href="#" data-placement="bottom" title="Twitter"><i class="icon-twitter icon-square"></i></a></li>
						<li><a href="#" data-placement="bottom" title="Linkedin"><i class="icon-linkedin icon-square"></i></a></li>
						<li><a href="#" data-placement="bottom" title="Pinterest"><i class="icon-pinterest icon-square"></i></a></li>
						<li><a href="#" data-placement="bottom" title="Google plus"><i class="icon-google-plus icon-square"></i></a></li>
					</ul>
				</div>
			</div>
		</div>
	</div>
	</footer>
</div>
<a href="#" class="scrollup"><i class="icon-chevron-up icon-square icon-32 active"></i></a>
<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="{{ asset('js/jquery.js') }}"><\/script>')</script>
{% block javascripts %}
	{% javascripts
		'js/bootstrap.js'
		'bundles/fosjsrouting/js/router.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/jquery.easing.1.3.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/jquery.jcarousel.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/jquery.fancybox.pack.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/jquery.fancybox-media.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/google-code-prettify/prettify.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/portfolio/jquery.quicksand.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/portfolio/setting.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/tweet/jquery.tweet.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/jquery.flexslider.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/modernizr.custom.79639.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/jquery.ba-cond.min.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/jquery.slitslider.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/modernizr.custom.28468.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/jquery.cslider.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/animate.js'
	    '@DatacityPublicBundle/Resources/public/js/theme/custom.js'
	   	'@DatacityPublicBundle/Resources/public/js/perso/modules.js'
	    %}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
	<script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
{% endblock %}
</body>
</html>
