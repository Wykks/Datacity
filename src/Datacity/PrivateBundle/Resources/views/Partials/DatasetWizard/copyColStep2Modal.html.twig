{% trans_default_domain "datasetWizard" %}
<div class="modal" tabindex="-1" role="dialog">
<div class="modal-dialog">
<div class="modal-content">
<div class="modal-header">
	<button type="button" class="close" ng-click="$hide()">&times;</button>
	<h4 class="modal-title">{{ 'datasetWizard.continue'|trans }} ?</h4>
</div>
<div class="modal-body">
	<p>{{ 'datasetWizard.replaceFields'|trans }}</p>
</div>
<div class="modal-footer">
	<button type="button" class="btn btn-default" ng-click="$hide()">{{ 'datasetWizard.cancel'|trans }}</button>
	<button type="button" class="btn btn-primary" ng-click="copyCol($hide)">{{ 'datasetWizard.next'|trans }}</button>
</div>
</div>
</div>
</div>