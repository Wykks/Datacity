{% verbatim %}
<div class="col-md-9">
	<div class="row">
		<div class="col-md-8 profile-info">
			<h1>{{user.firstName || "Hubert"}} {{user.lastName || "Bill"}}</h1>
			<p>
				{{user.description || "Aucune description"}}
			</p>
			<p>
				<a href="#">{{user.website || "www.mywebsite.com"}}</a>
			</p>
			<ul class="list-inline">
				<li>
					<i class="fa fa-map-marker"></i> {{user.city || "France" }}
				</li>
				<li>
					<i class="fa fa-calendar"></i> {{user.joinDate.date || "18 Janvier 1982" }}
				</li>
				<li>
					<i class="fa fa-briefcase"></i> {{user.activity || "Développeur FreeLance"}}
				</li>
				<li>
					<i class="fa fa-heart"></i> {{user.point || "12345"}} Point(s)
				</li>
			</ul>
		</div>
		<!--end col-md-8-->
		<div class="col-md-4">
			<div class="portlet sale-summary">
				<div class="portlet-title">
					<div class="caption">
						Bilan des activités
					</div>
					<div class="tools">
					</div>
				</div>
				<div class="portlet-body">
					<ul class="list-unstyled">
						<li>
							<span class="sale-info">
								Point(s)<i class="fa fa-img-up"></i>
							</span>
							<span class="sale-num">
								4
							</span>
						</li>
						<li>
							<span class="sale-info">
								Moyenne des notations<i class="fa fa-img-down"></i>
							</span>
							<span class="sale-num">
								50%
							</span>
						</li>
						<li>
							<span class="sale-info">
								Total des sources créés
							</span>
							<span class="sale-num">
								20
							</span>
						</li>
					</ul>
				</div>
			</div>
		</div>
		<!--end col-md-4-->
	</div>
	<!--end row-->
</div>
{% endverbatim %}